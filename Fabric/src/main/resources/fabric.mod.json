{
    "schemaVersion": 1,
    "id": "tinycoal",
    "version": "${version}",
    "name": "Tiny Coal",
    "description": "Allows you to convert coal or charcoal to Tiny Coal/Charcoal.",
    "authors": [
      "AzureDoom"
    ],
    "contact": {
      "homepage": "https://fabricmc.net/",
      "sources": "https://github.com/AzureDoom/TinyCoalML"
    },
    "license": "MIT",
    "icon": "icon.png",
    "environment": "*",
    "entrypoints": {
      "main": [
        "mod.azure.tinycoal.TinyCoalMod"
      ]
    },
    "mixins": [
      "tinycoal.mixins.json",
      "tinycoal.fabric.mixins.json"
    ],
    "depends": {
      "fabricloader": ">=0.14.21",
      "fabric": "*",
      "minecraft": ">=1.20.1",
      "java": ">=17"
    },
    "suggests": {
      "another-mod": "*"
    },
    "accessWidener": "tinycoal_common.accesswidener"
  }
  