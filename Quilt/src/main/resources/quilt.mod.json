{
  "schema_version": 1,
  "quilt_loader": {
    "group": "mod.azure.tinycoal",
    "id": "tinycoal",
    "version": "${version}",
    "metadata": {
      "name": "Tiny Coal",
      "description": "Allows you to convert coal or charcoal to Tiny Coal/Charcoal.",
      "contributors": {
        "AzureDoom": "Author"
      },
      "contact": {
        "sources": "https://github.com/AzureDoom/TinyCoalML"
      },
      "icon": "icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "mod.azure.tinycoal.TinyCoalMod"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.18.0-"
      },
      {
        "id": "minecraft",
        "versions": ">=1.19.3"
      },
      {
        "id": "quilt_base",
        "versions": ">=4.0.0-"
      }
    ]
  }
  }
  